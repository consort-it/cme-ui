{
  "name": "cme2",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "showcase": "ng serve --app=showcase --port=4201",
    "build": "ng build",
    "build:prod": "ng build --prod --namedChunks --subresource-integrity --output-hashing=all",
    "build-showcase:prod": "ng build --prod --app=showcase",
    "test": "ng test",
    "test:coverage": "ng test --single-run --watch=false --cc",
    "test:pact": "ng test --app=pact --config=apps/pact/karma-pact.conf.js --single-run=true",
    "test:pact:watch": "ng test --app=pact --config=apps/pact/karma-pact.conf.js",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "lint:watch": "nodemon --delay 2500ms --exec \"npm run lint || exit 1\" --ext ts,html,scss,json",
    "e2e": "ng e2e --app=cme-ui",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -n \"CME Workbench 2 documentation\" -d compodoc --disablePrivate",
    "generate-rest-clients": "bash ./generators/swagger/generateRestClients.sh",
    "postgenerate-rest-clients": "npm run format",
    "extract-translations": "bash ./generators/translations/extractTranslations.sh",
    "sonar": "./node_modules/.bin/sonar-scanner -Dsonar.host.url=https://cme.dev.k8s.consort-it.de/sonar/ -Dsonar.login=<token>",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "updateBuild": "node ./generators/build-number/replace.build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/cdk": "5.2.5",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/material": "5.2.5",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "5.2.10",
    "@ngrx/effects": "5.2.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "5.2.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@nrwl/nx": "1.0.3",
    "aws-amplify": "^0.3.3",
    "core-js": "^2.5.5",
    "mermaid": "^8.0.0-rc.8",
    "ng4-validators": "^5.1.0",
    "rxjs": "^5.5.10",
    "stacktrace-js": "^2.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.10",
    "@angular/language-service": "5.2.10",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@compodoc/compodoc": "^1.1.2",
    "@nrwl/schematics": "1.0.3",
    "@pact-foundation/karma-pact": "^2.1.5",
    "@pact-foundation/pact-node": "^6.16.0",
    "@pact-foundation/pact-web": "^5.9.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.60",
    "@types/stacktrace-js": "0.0.32",
    "codelyzer": "^4.3.0",
    "jasmine-core": "^2.9.1",
    "jasmine-marbles": "^0.2.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "nodemon": "^1.17.3",
    "prettier": "^1.12.1",
    "protractor": "~5.1.2",
    "replace-in-file": "^3.4.0",
    "semantic-release": "^15.1.8",
    "sonarqube-scanner": "^2.0.2",
    "ts-mockito": "^2.3.0",
    "ts-node": "~6.0.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
